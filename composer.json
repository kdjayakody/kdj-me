{
    "name": "kdj/php-frontend",
    "description": "Frontend for KDJ Authentication Service",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "KDJ Lanka (Pvt) Ltd",
            "email": "kdj@kdj.lk"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
